<body>

        <section class="login">
            <div class="container">

                <div class="card my-5">
                    <div class="card-header">
                        <div class="d p-5 border-bottom-0">
                            <h1 class="text-center fw-bold mb-0 fs-2">Sign up for free</h1>
                        </div>

                    
                    </div>
                    <div class="card-body">
                        <div class="modal-body pt-1 p-3">
                            <form class="" action="/login" method="post">
                            <div class="form-floating ">
                                <input name="usersname" type="text" class="form-control rounded-3" id="floatingInput" placeholder="username">
                                <label for="floatingInput">Username</label>
                            </div>
                            <div class="form-floating">
                                <input name="password" type="password" class="mt-3 form-control rounded-3" id="floatingPassword" placeholder="Password">
                                <label for="floatingPassword">Password</label>
                                <button class="mt-4 w-100 mb-2 btn btn-lg rounded-3 btn-success" type="submit">Sign up</button>
                            </div>
                           
                            </form>
                        </div>
                    </div>
                    <div class="card-footer text-center" >
                        
                            <small class="">You didn't have an account ? <a href="/register">Register</a></small>
                    </div>
                  </div>
            </div>
        </section>
    
